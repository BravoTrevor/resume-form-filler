{
  "manifest_version": 3,
  "name": "Simple Form Filler MVP",
  "version": "0.1.0",
  "description": "Fills basic fields (Name, Email, Phone) based on saved user data.",
  "permissions": [
    "storage",      
    "activeTab",    
    "scripting"     
  ],
  "background": {
    "service_worker": "background.js" 
  },
  "action": {
    "default_popup": "popup.html", 
    "default_icon": {              
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": { 
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
  }
}